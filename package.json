{
  "name": "vincentvandijck",
  "version": "1.0.0",
  "description": "Personal Website",
  "author": "Vincent van Dijck (https://www.vincentvandijck.nl)",
  "homepage": "https://github.com/vvandijck/vincentvandijck#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vvandijck/vincentvandijck"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production run-s build:*",
    "build:client": "nuxt generate",
    "build:guide": "build-storybook -c config/storybook -o dist/client/guide/ --quiet",
    "dev": "PORT=3000 nuxt",
    "guide": "start-storybook -s src/client/static -p 3001 -c config/storybook --ci",
    "lint": "run-s lint:*",
    "lint:js": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "lint:css": "stylelint '**/*.{vue,css}'",
    "lintfix": "run-s lintfix:*",
    "lintfix:js": "npm run lint:js -- --fix",
    "lintfix:css": "npm run lint:css -- --fix",
    "new-component": "node _scripts/new-component/index.js --component",
    "precommit": "npm run lint",
    "start": "nuxt start"
  },
  "dependencies": {
    "node-fetch": "2.6.0",
    "nuxt": "2.10.2",
    "vue": "2.6.11"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.7.6",
    "@storybook/addon-actions": "5.3.7",
    "@storybook/addon-knobs": "5.3.7",
    "@storybook/addons": "5.3.7",
    "@storybook/vue": "5.2.8",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.0.3",
    "babel-preset-vue": "2.0.2",
    "commander": "4.1.0",
    "eslint": "6.8.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-vue": "6.1.2",
    "lodash": "4.17.15",
    "npm-run-all": "4.1.5",
    "postcss-custom-properties": "9.0.2",
    "postcss-import": "12.0.1",
    "storybook-readme": "5.0.8",
    "stylelint": "13.0.0",
    "stylelint-config-standard": "19.0.0",
    "vue-svg-loader": "0.15.0",
    "vue-template-compiler": "2.6.11"
  }
}
